{
  "AuthGate": {
    "Url": "https://localhost:8081"
  },
  "Security": {
    "ForceHttps": false
  },
  "HttpClients": {
    "AuthGateApi": {
      "BaseUrl": "https://localhost:8081"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Information"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    }
  },
  "AuditRetention": {
    "Days": 7,
    "RunIntervalHours": 24
  },
  "Jobs": {
    "ContractActivation": {
      "IntervalSeconds": 60
    }
  }
}
